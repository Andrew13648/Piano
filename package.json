{
	"name": "@tonejs/piano",
	"version": "0.1.0",
	"description": "detailed piano using Salamander samples",
	"main": "build/piano.js",
	"scripts": {
		"help": "node scripts/help.js",
		"help:verbose": "node scripts/help.js -v",
		"compile": "tsc -p tsconfig.json",
		"compile:watch": "tsc -p tsconfig.json --watch",
		"build": "webpack -p --env.production",
		"build:watch": "webpack -p -w",
		"build:debug": "webpack --debug",
		"build:watch:debug": "webpack -w --debug",
		"compileAndBuild": "node scripts/compileAndBuild.js",
		"compileAndBuild:watch": "npm-run-all -p compile:watch build:watch",
		"compileAndBuild:debug": "npm run compileAndBuild -- -d",
		"compileAndBuild:watch:debug": "npm-run-all -p compile:watch build:watch:debug",
		"increment": "npm version v$(semver --increment $(npm show @tonejs/piano version)) --git-tag-version=false --allow-same-version",
		"test": "npm run compileAndBuild && npm run test:mocha",
		"test:debug": "npm run compileAndBuild:debug && npm run test:mocha:debug",
		"test:mocha": "mocha test/test.js --timeout 30000",
		"test:mocha:debug": "npm run test:mocha -- --dbg --timeout 0"
	},
	"files": [
		"README.md",
		"LICENSE",
		"build/piano.js.map",
		"build/piano.js",
		"src",
		"scripts"
	],
	"author": "Yotam Mann",
	"license": "MIT",
	"devDependencies": {
		"@types/node": "^12.6.2",
		"@types/tone": "git://github.com/Tonejs/TypeScript.git",
		"@typescript-eslint/eslint-plugin": "^1.11.0",
		"@typescript-eslint/parser": "^1.11.0",
		"babel-core": "^6.26.3",
		"babel-loader": "^7.1.4",
		"chai": "^4.1.2",
		"eslint": "^5.2.0",
		"mocha": "^5.2.0",
		"npm-run-all": "^4.1.5",
		"puppeteer": "^1.6.1",
		"semver": "^5.5.0",
		"static-server": "^2.2.1",
		"table": "^5.4.1",
		"tone": "^13.8.18",
		"ts-loader": "^6.0.4",
		"typescript": "^3.5.3",
		"webmidi": "^2.2.0",
		"webpack": "^4.8.3",
		"webpack-cli": "^2.1.3"
	},
	"keywords": [
		"Web Audio",
		"Tone.js",
		"Piano",
		"Sampler"
	]
}
